// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Provider {
  kakao
  google
}

enum Classify {
  en // 영어
  ch // 중국어
  jp // 일본어
}

model User {
  id       String   @id @unique @default(uuid())
  email    String   @unique
  provider Provider
  // 구글 or 카카오

  name         String @db.VarChar(5)
  nickname     String @unique @db.VarChar(10)
  phone        String @db.VarChar(13)
  profileImage String

  subMajor   SubMajor @relation(fields: [subMajorId], references: [id], onDelete: Cascade)
  subMajorId String
  //세부전공 -> major 전공학과 relation

  thermometer Int @default(0)
  // 온도계

  top Int @default(100)
  // 온도계 기준 상위 몇퍼센트 초기값 100%

  interestKeyword UserInterest[]
  // 관심분야 키워드 

  communities   Community[]
  comments      Comment[]
  communityLike CommunityLike[]
  commentLike   CommentLike[]

  // 스크랩 
  scrapCompetition ScrapCompetition[]
  scrapOutside     ScrapOutside[]
  scrapLanguage    ScrapLanguage[]
  scrapQNet        ScrapQNet[]
  scrapIntern      ScrapIntern[]

  // 활동내역
  userCompetition UserCompetition[]
  userOutside     UserOutside[]
  userQnet        UserQnet[]
  userIntern      UserIntern[]
  userLanguage    UserLanguage[]
}

// !(--------------------------------------------------------------)

model Community {
  id              String          @id @unique @default(uuid())
  category        String          @db.VarChar(20)
  title           String          @db.VarChar(30)
  detail          String          @db.LongText
  view            Int             @default(0)
  like            Int             @default(0)
  comment         Int             @default(0)
  date            DateTime        @default(dbgenerated("NOW()")) @db.DateTime //생성시간
  userId          String?
  user            User?           @relation(fields: [userId], references: [id], onDelete: Cascade)
  communitiyLikes CommunityLike[]
  comments        Comment[]
}

model Comment {
  id          String        @id @unique @default(uuid())
  comment     String        @db.VarChar(100)
  date        DateTime      @default(dbgenerated("NOW()")) @db.DateTime
  userId      String?
  user        User?         @relation(fields: [userId], references: [id], onDelete: Cascade)
  communityId String?
  community   Community?    @relation(fields: [communityId], references: [id], onDelete: Cascade)
  commentLike CommentLike[]
}

model CommunityLike {
  id          String     @id @unique @default(uuid())
  userId      String
  user        User       @relation(fields: [userId], references: [id], onDelete: Cascade)
  communityId String?
  community   Community? @relation(fields: [communityId], references: [id], onDelete: Cascade)
}

model CommentLike {
  id        String   @id @unique @default(uuid())
  userId    String?
  user      User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  commentId String?
  comment   Comment? @relation(fields: [commentId], references: [id], onDelete: Cascade)
}

model Interest {
  id       String @id @unique @default(uuid())
  interest String @unique
  // 관심분야(공모전 or 대외활동 or 자격증 or 어학 or 인턴)

  keywords      Keyword[]
  userInterests UserInterest[]
}

model Keyword {
  id      String @id @unique @default(uuid())
  keyword String @unique
  // 키워드(공모전 or 대외활동 or 자격증 or 어학 or 인턴)

  interests     Interest[]
  userInterests UserInterest[]
}

model UserInterest {
  id         String   @id @unique @default(uuid())
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId     String
  interest   Interest @relation(fields: [interestId], references: [id], onDelete: Cascade)
  interestId String
  keyword    Keyword  @relation(fields: [keywordId], references: [id], onDelete: Cascade)
  keywordId  String
}

// 유저가 가지는 Interest : [ Keyword : [] ] 형태로 저장
// !(--------------------------------------------------------------)

// 전공학과
model MainMajor {
  id        String     @id @unique @default(uuid())
  mainMajor String     @unique
  subMajor  SubMajor[]
}

//세부 전공
model SubMajor {
  id          String    @id @unique @default(uuid())
  subMajor    String    @unique
  mainMajor   MainMajor @relation(fields: [mainMajorId], references: [id], onDelete: Cascade)
  mainMajorId String
  user        User[]
}

// 스크랩
model ScrapCompetition {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  competitionId String
  user          User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String
}

model ScrapOutside {
  id        String   @id @unique @default(uuid())
  createdAt DateTime @default(dbgenerated("NOW()")) @db.DateTime
  outsideId String
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId    String
}

model ScrapLanguage {
  id         String   @id @unique @default(uuid())
  createdAt  DateTime @default(dbgenerated("NOW()")) @db.DateTime
  languageId String
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId     String
}

model ScrapQNet {
  id        String   @id @unique @default(uuid())
  createdAt DateTime @default(dbgenerated("NOW()")) @db.DateTime
  qNetId    String
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId    String
}

model ScrapIntern {
  id        String   @id @unique @default(uuid())
  createdAt DateTime @default(dbgenerated("NOW()")) @db.DateTime
  internId  String
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId    String
}

// !(--------------------------------------------------------------)

// 활동 내역 (온도계를 구현하기위한 테이블)
// 유저가 작성하기 때문에, 실제 DB데이터가 아닐수도 있다.
// field          활동분야 (공모전 or 대외활동 or 자격증 or 어학 or 인턴)
// category       분야 선택(#기획/아이디어)
// activeTitle    활동명
// activeContent  활동내용
model UserCompetition {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  field         String
  category      String
  activeTitle   String
  activeContent String
  user          User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String?
}

model UserOutside {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  field         String
  category      String
  activeTitle   String
  activeContent String
  user          User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String?
}

model UserQnet {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  field         String
  category      String
  activeTitle   String
  activeContent String
  user          User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String?
}

model UserIntern {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  field         String
  category      String
  activeTitle   String
  activeContent String
  period        String
  user          User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String?
}

model UserLanguage {
  id            String   @id @unique @default(uuid())
  createdAt     DateTime @default(dbgenerated("NOW()")) @db.DateTime
  field         String
  category      String
  activeTitle   String
  activeContent String
  score         String
  user          User?    @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId        String?
}

// !(--------------------------------------------------------------)

